{
  "manifest_version": 3,
  "name": "Custom Cursor",
  "version": "1.0",
  "permissions": ["storage","scripting","activeTab"],
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ]
}